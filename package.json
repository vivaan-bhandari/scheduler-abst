{
  "name": "abst-fullstack",
  "version": "1.0.0",
  "description": "ABST - Acuity Based Staffing Tool",
  "scripts": {
    "install-all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "start-backend": "cd backend && python manage.py runserver",
    "start-frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "build": "echo 'Building frontend...' && cd frontend && npm run build",
    "build-frontend": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["abst", "healthcare", "staffing", "django", "react"],
  "author": "ABST Team",
  "license": "MIT"
} 